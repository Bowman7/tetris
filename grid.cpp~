#include"grid.h"

//grid constructor
Grid::Grid(){
  n_row = 20;
  n_col = 10;
  cellSize = 30;
}

//initialize grid values
void Grid::Initialize(){
  for(int row =0;row<n_row;row++){
    for(int col=0;col<n_col;col++){
      grid[row][col] = 0;
    }
  }
}
